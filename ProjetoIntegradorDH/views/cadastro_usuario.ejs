<%- include('partials/header.ejs'); %>

        <h1 class="Cadastro">Cadastro de Usuário <span class="Prod">Prod</span><span class="Market">Market</span></h1>
        <hr>

        <form class="form-user" action="#" method="post" enctype="multipart/form-data">

            <div class="form-group form-check col-md-12">
                    <input type="checkbox" class="form-check-input" id="checkComprar" name="checkComprar" value="Comprar">
                    <label class="form-check-label" for="checkComprar"> Comprar</label>
                    
                    <input type="checkbox" class="form-check-input" id="checkVender" name="checkVender" value="Vender">
                    <label class="form-check-label" for="checkVender"> Vender</label>
            </div>

            <div class="form-row">
                <div class="form-group col-md-6">
                    <label for="nome">Nome Empresarial</label>
                    <input type="text" class="form-control" id="nome" name="nome" placeholder="Nome">
                </div>

                <div class="form-group col-md-6">
                    <label for="nome_fantasia">Titulo do Estabelecimento (Nome de Fantasia) </label>
                    <input type="text" class="form-control" id="nome_fantasia" name="nome_fantasia" placeholder="Nome de Fantasia">
                </div>

            </div>

            <div class="form-row">
                <div class="form-group col-md-4">
                    <label for="cnpj">CNPJ</label>
                    <input type="text" class="form-control" id="cnpj" name="cnpj" placeholder="Digite o CNPJ">
                </div>

                <div class="form-group col-md-4">
                    <label for="ie">Incrição Estadual</label>
                    <input type="text" class="form-control" id="ie" name="ie" placeholder="Digite a Inscrição Estadual">
                </div>

                <div class="form-group col-md-4">
                    <label for="ramo">Ramo de Atividade</label>
                    <input type="text" class="form-control" id="ramo-user" name="ramo" placeholder="Atividade Empresarial">
                </div>
            </div>

            <div class="form-row">
                <div class="form-group col-md-6">
                    <label for="codigo_ap">Código e Descição da Atividade Econômica Principal</label>
                    <input type="text" class="form-control" id="codigo_ap" name="codigo_ap" placeholder="Atividade Principal">
                </div>

                <div class="form-group col-md-6">
                    <label for="codigo_as">Código e Descição da Atividade Econômica Secundária</label>
                    <input type="text" class="form-control" id="codigo_as" name="codigo_as" placeholder="Atividade Secundária">
                </div>
            </div>

            <div class="form-row">
                <div class="form-group col-md-4">
                    <label for="codigo_natureza">Codigo e Descição da Natureza Jurídica</label>
                    <input type="text" class="form-control" id="codigo_natureza" name="codigo_natureza" placeholder="Natureza Jurídica">
                </div>

                <div class="form-group col-md-3">
                    <label for="responsavel">Nome do Responsável</label>
                    <input type="text" class="form-control" id="responsavel" name="responsavel" placeholder="Insira seu nome">
                </div>

                <div class="form-group col-md-3">
                    <label for="email">E-mail do Responsável</label>
                    <input type="text" class="form-control" id="email" name="email" placeholder="Insira seu E-mail">
                </div>

                <div class="form-group col-md-2">
                    <label for="telefone">Telefone</label>
                    <input type="text" class="form-control" id="telefone" name="telefone" placeholder="99999-9999">
                </div>
            </div>

            <div class="form-row">
                <div class="form-group col-md-2">
                    <label for="cep">Informe o CEP</label>
                    <input type="text" class="form-control" id="cep" name="cep" placeholder="CEP">
                </div>

                <div class="form-group col-md-2">
                    <label for="estado" class="col-sm-2 control-label">Estado</label>
                    <div class="col-auto">
                        <select name="estado" id="estado" class="form-control">

                            <option value="ac">Acre</option>
                            <option value="al">Alagoas</option>
                            <option value="am">Amazonas</option>
                            <option value="ap">Amapá</option>
                            <option value="ba">Bahia</option>
                            <option value="ce">Ceará</option>
                            <option value="df">Distrito Federal</option>
                            <option value="es">Espírito Santo</option>
                            <option value="go">Goiás</option>
                            <option value="ma">Maranhão</option>
                            <option value="mt">Mato Grosso</option>
                            <option value="ms">Mato Grosso do Sul</option>
                            <option value="mg">Minas Gerais</option>
                            <option value="pa">Pará</option>
                            <option value="pb">Paraíba</option>
                            <option value="pr">Paraná</option>
                            <option value="pe">Pernambuco</option>
                            <option value="pi">Piauí</option>
                            <option value="rj">Rio de Janeiro</option>
                            <option value="rn">Rio Grande do Norte</option>
                            <option value="ro">Rondônia</option>
                            <option value="rs">Rio Grande do Sul</option>
                            <option value="rr">Roraima</option>
                            <option value="sc">Santa Catarina</option>
                            <option value="se">Sergipe</option>
                            <option value="sp">São Paulo</option>
                            <option value="to">Tocantins</option>
                        </select>
                    </div>
                </div>

                <div class="form-group col-md-6">
                    <label for="endereco">Lograduro</label>
                    <input type="text" class="form-control" id="endereco" name="endereco" placeholder="Digite o Endereço">
                </div>

                <div class="form-group col-md-2">
                    <label for="complemento">Complemento</label>
                    <input type="text" class="form-control" id="complemento" name="complemento" placeholder="Número / Bloco">
                </div>

                <div class="form-group col-md-5">
                    <label for="senha">Senha</label>
                    <input type="password" class="form-control" id="senha" name="senha" placeholder="Cadastre sua Senha">
                </div>
            
            <div class="form-row">
                <div class="form-group col-md-7">
                    <label for="inputImagem">Cadastrar Imagem <br>( Tamanho mínimo 400x400px, formato JPG, máximo 07
                            imagens
                            )</label>
                    <input type="file" id="inputImagem" name="myfile" multiple>
                </div>
            </div>

            </div>

            <div class="form-group col-md-2">
                <button type="submit" class="btn btn-primary">Cadastrar</button>
            </div>

        </form>

        <%- include('partials/footer.ejs'); %>